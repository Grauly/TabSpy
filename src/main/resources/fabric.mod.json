{
  "schemaVersion": 1,
  "id": "tabspy",
  "version": "${version}",
  "name": "TabSpy",
  "description": "Allows for a separate player list, containing players specified by a regex.",
  "authors": [
    "Grauly"
  ],
  "contact": {
    "repo": "https://github.com/grauly/TabSpy"
  },
  "license": "MIT",
  "icon": "assets/tabspy/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "grauly.tabspy.client.TabSpyClient"
    ],
    "main": [
      "grauly.tabspy.TabSpy"
    ],
    "modmenu": [
      "grauly.tabspy.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "TabSpy.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": ">=15.0.12",
    "modmenu": ">=11.0.1"
  }
}
